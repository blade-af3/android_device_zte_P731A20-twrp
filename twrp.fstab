# mount point   fstype  device                                                  [device2]                                       fstype2
/boot           emmc    /dev/block/platform/sprd-sdhci.3/by-name/boot
/system         ext4    /dev/block/platform/sprd-sdhci.3/by-name/system
/cache          ext4    /dev/block/platform/sprd-sdhci.3/by-name/cache
/data           ext4    /dev/block/platform/sprd-sdhci.3/by-name/userdata       length=-16384
/recovery       emmc    /dev/block/platform/sprd-sdhci.3/by-name/recovery

/external_sd    vfat    /dev/block/platform/sprd-sdhci.0/by-num/p1    /dev/block/platform/sprd-sdhci.0/by-num/p1    flags=display="MicroSD";storage;wipeingui;removable